# function retur all numerical value of lis
def num(a):
    b=[]
    for i in a : # || | will nor work onlr or works
        if type(i)==list:
            for j in i :
                 if(type(j)==float or type(j)==int):           
                     b.append(i);           
        if(type(i)==float or type(i)==int):
            b.append(i);
    return b            


